{
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/docs",
            "dest": "/api/index.py"
        },
        {
            "src": "/openapi.json",
            "dest": "/api/index.py"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "PYTHONPATH": "backend"
    }
}